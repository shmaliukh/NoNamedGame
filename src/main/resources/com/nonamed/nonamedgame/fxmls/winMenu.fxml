<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            fx:id="winScreen" style="-fx-background-color: GRAY;" stylesheets="@winMenuStyle.css"
            xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.nonamed.nonamedgame.scenes.WinMenuController">
    <children>
        <Button fx:id="tryAgain" layoutX="400.0" layoutY="215.0" mnemonicParsing="false" onAction="#onRetryButtonClick"
                prefHeight="100.0" prefWidth="400.0" styleClass="buttonTryAgain" text="Try again"
                AnchorPane.leftAnchor="400.0" AnchorPane.rightAnchor="400.0">
            <font>
                <Font size="50.0"/>
            </font>
        </Button>
        <Button fx:id="exitButton" layoutX="400.0" layoutY="482.0" mnemonicParsing="false"
                onAction="#onMainMenuButtonClick" prefHeight="100.0" prefWidth="400.0" styleClass="buttonMenu"
                text="Main menu" AnchorPane.leftAnchor="400.0" AnchorPane.rightAnchor="400.0">
            <font>
                <Font size="50.0"/>
            </font>
        </Button>
        <Text layoutX="462.0" layoutY="121.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Your are win!">
            <font>
                <Font size="50.0"/>
            </font>
        </Text>
    </children>
</AnchorPane>
